{
    "name":"Arduino/Genuino Zero",
    "target":"genuino_zero",
    "cc":"armgcc",
    "classes":["genuino_zero.GenuinoZero"],
    "ram":32,
    "flash":256,
    "family_type":"armcmx",
    "family_name":"atmelsamd21",
    "chip_model":"SAMD21",
    "manual_reset": 1,
    "preferred_baud":115200,
    "board_producer":"Arduino",
    "chip_manifacturer":"Atmel",
    "architecture":"cortex-m0plus",
    "gccopts":{
        "cflags": ["-mcpu=cortex-m0plus","-mthumb", "-O2","-fomit-frame-pointer","-falign-functions=16","-ffunction-sections", "-fdata-sections", "-fno-common", "-Wall", "-Wextra", "-Wstrict-prototypes","-nostdlib","-std=c99"],
        "defs": ["__ATSAMD21G18AU__","SAMD21_VHAL","VM_DEFAULT_THREAD_SIZE=1024"]
    },
    "relocator":"cortex-m",
    "type":"board",
    "virtualizable":"GenuinoZero",
    "uplink_reset": true,
    "reset_time": 2000,
    "has_alter_ego":false,
    "connection":{
        "baudrate":115200,
        "parity":"n",
        "bytesize":8,
        "stopbits":1,
        "dsrdtr":false,
        "rtscts":false
    }
}
